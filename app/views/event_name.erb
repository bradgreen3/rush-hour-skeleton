<div id="sources">
  <div class='col-md-12 navbar navbar-default navbar-static-top navbar-inverse'>
    <a href='/' class='navbar-brand'>Rush Hour</a>
    <ul class='nav navbar-nav navbar-right'>
      <li><a href = "https://www.youtube.com/watch?v=ZFq_TFyhzmo">Full Movie</a></li>
      <li><a href = "https://www.redbubble.com/shop/jackie+chan">Store</a></li>
      <li class='dropwdown'>
        <a href = '#' class='dropdown-toggle' data-toggle='dropdown'>Sources
        <span class="caret"></span></a>
        <ul class='dropdown-menu'>
          <% @sources.each do |client| %>
            <li><a href="/sources/<%= client.identifier %>"><%= client.identifier %></a></li>
          <% end %>
        </ul>
      </li>
    </ul>
  </div>
</div>

<div class="event">
<h1> Event: <%= @eventname %> </h1>
<table class="table-center">
  <caption class="back"><a href="/"><<</a></caption>
  <tr>
    <th>Hour (Military)</th>
    <th>Frequency</th>
  </tr>
  <% @data.each do |key, value|%>
  <tr>
    <td><%=key%></td>
    <td><%=value%></td>
  </tr>
  <% end %>
  <tr>
    <td>Total</td>
    <td><%= @total %></td>
  </tr>
</table>
</div>
